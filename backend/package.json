{
  "name": "@spa-hono/backend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "dev:test": "NODE_ENV=test DATABASE_MODE=memory tsx watch ./src/server.ts",
